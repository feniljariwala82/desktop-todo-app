{
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "alias": {
          "components": "./app/src/components",
          "core": "./app/src/core",
          "pages": "./app/src/pages",
          "partials": "./app/src/partials"
        }
      },
      "@babel/plugin-syntax-dynamic-import"
    ]
  ],
  "presets": ["@babel/preset-env", "@babel/preset-react"]
}
